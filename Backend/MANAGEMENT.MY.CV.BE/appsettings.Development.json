{
  "SerilogConfig": {
    "EnableConsoleLog": true,

    "EnableFileLog": false,
    "PathFileLog": "./logs/.json",
    "EnableElasticsearchLog": false,
    "ElasticsearchUri": "http://localhost:9200",
    "ElasticsearchIndexFormat": "hr-management-api-{0:yyyy.MM.dd}",
    "ElasticsearchUsername": "admin"
  },
  "CacheConfig": {
    "UseRedis": false,
    "RedisUri": "localhost:6379",
    "RedisInstanceName": "management-my-cv-api-"
  },
  "JwtConfig": {
    "Issuer": "SYSTEM",
    "Audience": "SYSTEM",
    "AccessTokenExpiration": 5,
    "RefreshTokenExpiration": 10
  }
}
